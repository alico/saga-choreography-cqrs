{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMqSettings": [
    {
      "Name": "MainHost",
      "RabbitMqHostUrl": "localhost",
      "Username": "guest",
      "Password": "guest",
      "RetryCount": 2,
      "RetryTimeInterval": 1000,
      "VirtualHost": "/",
      "PrefetchCount": 1,
      "TrackingPeriod": 1,
      "TripThreshold": 50,
      "ActiveThreshold": 50,
      "ResetInterval": 1
    }
  ]
}
